{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
     "parserOptions": {
         "parser": "babel-eslint"
     },
    "extends": [
        "airbnb-base",
        "plugin:vue/recommended"
    ],
    "rules": {},
    "overrides": [
        {
        "files": ["*"],
        "rules": {
            "max-len": "off",
            "no-param-reassign": "off"
            }
        }
    ]
}